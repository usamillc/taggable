<div class="modal fade" id="editAttributeModal-<%= attribute.id %>" tabindex="-1" role="dialog" aria-labelledby="modalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modalLabel">属性の名称変更</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <%= form_for(attribute) do |f| %>
        <div class="modal-body">
          <div class="form-group row">
            <%= f.label '属性名', class: 'col-sm-2 col-form-label' %>
            <%= f.text_field :screenname, class: 'col-sm-9 form-control' %>
          </div>
          <div class="form-group row">
            <%= f.label '英表記', class: 'col-sm-2 col-form-label' %>
            <%= f.text_field :name, class: 'col-sm-9 form-control' %>
          </div>
        </div>
        <div class="modal-footer">
          <%= f.submit '変更', class: "btn btn-primary" %>
        </div>
      <% end %>
    </div>
  </div>
</div>
